{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style":"off",
        "prefer-const":"off",
        "indent":"off",
        "eol-last":"off", 
        "space-before-blocks":"off"
    }
}
